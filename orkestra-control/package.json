{
    "name": "orkestra-control",
    "version": "0.0.1",
    "private": true,
    "sideEffects": false,
    "dependencies": {
        "@angular/animations": "9.0.7",
        "@angular/cdk": "8.2.3",
        "@angular/common": "9.0.7",
        "@angular/compiler": "9.0.7",
        "@angular/core": "9.0.7",
        "@angular/flex-layout": "9.0.0-beta.31",
        "@angular/forms": "9.0.7",
        "@angular/localize": "^9.1.12",
        "@angular/material": "8.2.3",
        "@angular/platform-browser": "9.0.7",
        "@angular/platform-browser-dynamic": "9.0.7",
        "@angular/router": "9.0.7",
        "@biesbjerg/ngx-translate-extract": "^7.0.4",
        "@ngx-translate/core": "^13.0.0",
        "@ngx-translate/http-loader": "^6.0.0",
        "@webcomponents/webcomponentsjs": "^2.4.3",
        "angular-in-memory-web-api": "0.9.0",
        "jasmine-core": "3.5.0",
        "jasmine-marbles": "0.6.0",
        "ng-chat": "3.0.3",
        "orkestraLib": "file:///../libs/orkestraLib",
        "rxjs": "6.5.5",
        "share-api-polyfill": "^1.0.20",
        "tslib": "1.11.1",
        "zone.js": "0.10.3"
    },
    "engines": {
        "node": ">=14.18.1"
    },
    "scripts": {
        "ng": "ng",
        "postinstall": "ngcc --properties es2015 browser module main --async false",
        "start": "ng serve --configuration --ssl",
        "build": "node --max_old_space_size=4096 node_modules/@angular/cli/bin/ng build  --prod  --vendor-chunk --common-chunk --optimization --output-path ../public/",
        "build:dev": "node --max_old_space_size=4096 node_modules/@angular/cli/bin/ng build  --configuration development  --vendor-chunk --common-chunk --optimization --output-path ../public/",
        "test": "ng test",
        "lint": "ng lint",
        "e2e": "ng e2e",
        "i18n:init": "ngx-translate-extract --input ./src --output ./src/assets/i18n/template.json --key-as-default-value --replace --format json",
        "prettier": "prettier --ignore-path .gitignore \"**/*.+(ts|js|json)\"",
        "format": "npm run prettier -- --write"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "~0.901.1",
        "@angular/cli": "~9.0.7",
        "@angular/compiler-cli": "~9.0.7",
        "@angular/language-service": "~9.0.7",
        "@types/jasmine": "~3.5.0",
        "@types/jasminewd2": "~2.0.3",
        "@types/node": "^12.11.1",
        "codelyzer": "^5.1.2",
        "jasmine-core": "~3.5.0",
        "jasmine-spec-reporter": "~4.2.1",
        "karma": "~4.4.1",
        "karma-chrome-launcher": "~3.1.0",
        "karma-coverage-istanbul-reporter": "~2.1.0",
        "karma-jasmine": "~3.0.1",
        "karma-jasmine-html-reporter": "^1.4.2",
        "keycloak-angular": "7.3.1",
        "keycloak-js": "8.0.0",
        "prettier": "^2.4.1",
        "protractor": "~5.4.3",
        "ts-node": "~8.3.0",
        "tslint": "~6.1.0",
        "typescript": "~3.6.4"
    }
}
